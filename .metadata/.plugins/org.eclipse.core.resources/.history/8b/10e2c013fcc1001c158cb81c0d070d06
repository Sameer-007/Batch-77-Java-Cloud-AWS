import org.junit.Test;

public class CalculatorTest {
  
	
	@Test  // Test Case 1
	public void testAddition() {
		
		Calculator cal=new Calculator();
		int result=cal.addition(10, 10);
		
		assertEquals(20, result);
		assertEquals(100, cal.addition(50, 50));
	}
	 
