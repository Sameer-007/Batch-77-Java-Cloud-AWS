package com.wipro.fundamentals;

public class LoopingEx {
	public static void main(String[] args) {
		// Print Numbers 1 - 10
		for(int i=1;i<=10;i=i+2) {
			System.out.println(i);
		}
		
		// print Sum of numbers from 1-10
		int sum=0;
		for(int i=1;i<=10;i++) {
			sum=sum+i;
		}
	    
		System.out.println("Sum " + sum);
		
	}

}
