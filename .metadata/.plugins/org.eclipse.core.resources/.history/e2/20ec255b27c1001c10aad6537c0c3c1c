	public static void main(String[] args) {
		// Array is Fixed size
		// Array is an Object
		double[] cgpa=new double[100];
		cgpa[0]=6.7;
		cgpa[1]=7.7;
		
		String[] city=new String[] {"Chennai","Kollam","Madikeri","Vizag","Pune"};
		
		 
		
		
	}
	
	
	

