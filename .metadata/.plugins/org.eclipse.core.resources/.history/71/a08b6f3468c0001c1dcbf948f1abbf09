package com.wipro.arrayex;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        // Single Dimensional Array - used to store the elements in continuous memory location 
    	// Declare Array
    	int[] marks=new int[6];  //Variable must provide either dimension expressions or an array initializer
    	marks[0]=77;
    	marks[1]=88;
    	marks[2]=66;
    	marks[3]=55;
    	marks[4]=78;
    	marks[5]=87;
    	
    	
    	
    	System.out.println(marks[0]);
    	
    	
    }
}
