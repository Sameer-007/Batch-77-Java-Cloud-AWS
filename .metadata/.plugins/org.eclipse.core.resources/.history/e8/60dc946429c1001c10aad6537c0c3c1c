package com.wipro.arrayex;

public class ArrayEx {
	
	public static void main(String[] args) {
		// Array is Fixed size
		// Array is an Object
		double[] cgpa=new double[100];
		cgpa[0]=6.7;
		cgpa[1]=7.7;
		
		String[] city=new String[] {"Chennai","Kollam","Madikeri","Vizag","Pune"};
		
		for(int i=0;i<city.length;i++) {
			System.out.println(city[i]);
		}
		 System.out.println("--- For Each loop --- ");
		// for each - works only with object
		for( String val : city ) {
			// do the operation
			System.out.println(val);
		}
		
		
	}
	
	
	

}
